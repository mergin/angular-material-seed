{
    "name": "angular-material-seed",
    "version": "0.0.0",
    "engines": {
        "node": ">=20 <=23",
        "npm": ">=10 <=11"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:aot": "ng build --aot",
        "clean": "rm -rf package-lock.json dist/ node_modules/",
        "clean:i": "npm run clean && npm install",
        "rebuild": "npm run clean:i && npm run build:aot",
        "watch": "ng build --watch --configuration development",
        "test": "ng test --code-coverage",
        "serve:ssr:angular-material-seed": "node dist/angular-material-seed/server/server.mjs",
        "i18n": "ng extract-i18n --output-path src/locale",
        "prepare": "husky"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^19.1.6",
        "@angular/cdk": "^19.1.4",
        "@angular/common": "^19.1.6",
        "@angular/compiler": "^19.1.6",
        "@angular/core": "^19.1.6",
        "@angular/forms": "^19.1.6",
        "@angular/material": "^19.1.4",
        "@angular/platform-browser": "^19.1.6",
        "@angular/platform-browser-dynamic": "^19.1.6",
        "@angular/platform-server": "^19.1.6",
        "@angular/router": "^19.1.6",
        "@angular/ssr": "^19.1.7",
        "@ngrx/store": "^19.0.1",
        "express": "^4.21.2",
        "ngxtension": "^4.3.2",
        "rxjs": "~7.8.1",
        "tslib": "^2.8.1",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^19.1.7",
        "@angular/cli": "^19.1.7",
        "@angular/compiler-cli": "^19.1.6",
        "@angular/localize": "^19.1.6",
        "@commitlint/cli": "^19.7.1",
        "@commitlint/config-conventional": "^19.7.1",
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.20.0",
        "@ngrx/eslint-plugin": "^19.0.1",
        "@types/express": "^5.0.0",
        "@types/jasmine": "~5.1.6",
        "@types/node": "^22.13.4",
        "@typescript-eslint/eslint-plugin": "^8.24.0",
        "@typescript-eslint/parser": "^8.24.0",
        "eslint": "^9.20.1",
        "globals": "^15.15.0",
        "husky": "^9.1.7",
        "jasmine-core": "~5.6.0",
        "karma": "~6.4.4",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "msw": "^2.7.0",
        "typescript": "~5.7.3"
    },
    "msw": {
        "workerDirectory": [
            "src"
        ]
    }
}
